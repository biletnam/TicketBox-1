@using TicketBox.Domain.Entities;
@model IEnumerable<Event>

@{
    ViewBag.Title = "Мероприятия";
}

@foreach (var p in @Model)
{
    <div>
        <h3>@p.EventId</h3>
        <h3>@p.Name</h3>
        <p>@p.Description</p>
        <p>@p.Location</p>
        <p>@p.TimeEvent</p>
        <p>@p.SpecialEvent</p>
        <p>@p.Type.Name</p>
    </div>
}

